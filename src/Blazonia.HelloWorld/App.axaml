<local:AppBase
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="Blazonia.HelloWorld.App"
    xmlns:local="using:Blazonia.HelloWorld"
	xmlns:nav="using:Blazonia.Navigation"
	xmlns:helloWorld="clr-namespace:Blazonia.HelloWorld"
    xmlns:semi="https://irihi.tech/semi"
	RequestedThemeVariant="Default">
	<!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<helloWorld:AppBase.DataTemplates>

	</helloWorld:AppBase.DataTemplates>

	<local:AppBase.Styles>
		<semi:SemiTheme Locale="zh-CN" />
	</local:AppBase.Styles>
</local:AppBase>